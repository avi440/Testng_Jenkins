   <build>
    <plugins>
		<plugin> 
			<artifactId>maven-resources-plugin</artifactId>
			 <version>3.0.1</version>
			  <executions>
				<execution>
				 <id>copy-resources</id> 
				 <phase>validate</phase>
				   <goals> 
					<goal>copy-resources</goal> 
				   </goals> 
					 <configuration>
						  <outputDirectory>C:\Project\workspace\rest-api\framework</outputDirectory> 
					  <resources> 
						<resource>
							 <directory>${suiteFileNames}</directory> <!--source to your testng.xml--> 
							 <filtering>true</filtering> 
					    </resource>
					  </resources> 
					 </configuration>
				 </execution> 
				</executions> 
		 </plugin> 
    </plugins>
  </build>



